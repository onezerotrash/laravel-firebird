{
    "name": "onezerotrash/laravel-firebird",
    "description": "Firebird database driver for the Laravel Framework",
    "license": "MIT",
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/support": "^8.65|^9.0|^10.0",
        "illuminate/container": "^8.65|^9.0|^10.0",
        "illuminate/database": "^8.65|^9.0|^10.0",
        "illuminate/events": "^8.65|^9.0|^10.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.4",
        "phpunit/phpunit": "^9.5.10|^10.0",
        "orchestra/testbench": "^6.19|^7.0|^8.0.8",
        "fakerphp/faker": "^1.15"
    },
    "archive": {
        "exclude": [
            "/tests"
        ]
    },
    "autoload": {
        "psr-4": {
            "onezerotrash\\Firebird\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "onezerotrash\\Firebird\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "onezerotrash\\Firebird\\FirebirdServiceProvider"
            ]
        }
    }
}
